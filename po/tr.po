#: ../libxfce4kbd-private/xfce-shortcut-dialog.c:213
msgid "Command:"
msgstr "Komut:"

#: ../libxfce4kbd-private/xfce-shortcut-dialog.c:217
msgid "Shortcut"
msgstr "Kısayol"

#: ../libxfce4kbd-private/xfce-shortcut-dialog.c:258
msgid "Shortcut:"
msgstr "Kısayol:"

